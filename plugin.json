{
    "version"           :   "1.0.7",
    "name"              :   "ETCeres",
    "pluginIcon"        :   "icon_plugin_xs.png",
    "authorIcon"        :   "icon_author_xs.png",
    "email"             :   "sales@plentymarkets.com",
    "phone"             :   "+49 561 98 681 100",
    "description"       :   "A Coconut theme for Ceres 3.",
    "namespace"         :   "ETCeres",
    "author"            :   "plentysystems AG",
    "type"              :   "theme",
    "require"           :   {
                                "Ceres":">= 3.0.0"
                            },
    "serviceProvider"   :   "ETCeres\\Providers\\CeresCoconutServiceProvider",
    "dataProviders"     :
    [
        {
            "key"           :"ETCeres\\Containers\\CeresCoconutContainer",
            "name"          :"Stylesheet",
            "description"   :"Add changes to the CSS file"
        },
        {
            "key"           :"ETCeres\\Containers\\CeresCoconutItemListContainer1",
            "name"          :"First item list",
            "description"   :"Display a configurable list of items in your online store"
        },
        {
            "key"           :"ETCeres\\Containers\\CeresCoconutItemListContainer2",
            "name"          :"Second item list",
            "description"   :"Display a configurable list of items in your online store"
        },
        {
            "key"           :"ETCeres\\Containers\\CeresCoconutItemListContainer3",
            "name"          :"Third item list",
            "description"   :"Display a configurable list of items in your online store"
        },
        {
            "key"           :"ETCeres\\Containers\\CeresCoconutMobileNavigationVueTemplateContainer",
            "name"          :"Container for MobileNavigation Component Template",
            "description"   :"Container for MobileNavigation Component Template",
            "defaultLayoutContainer": "Ceres::Script.AfterScriptsLoaded"
        },
        {
            "key"           :"ETCeres\\Containers\\CeresCoconutCategoryItemVueTemplateContainer",
            "name"          :"Container for CategoryItem Component Template",
            "description"   :"Container for CategoryItem Component Template",
            "defaultLayoutContainer": "Ceres::Script.AfterScriptsLoaded"
        },
        {
            "key"           :"ETCeres\\Containers\\CeresCoconutItemImageCarousel",
            "name"          :"Container for ImageCarousel Component Template",
            "description"   :"Container for ImageCarousel Component Template",
            "defaultLayoutContainer": "Ceres::Script.AfterScriptsLoaded"
        }
    ]
}
