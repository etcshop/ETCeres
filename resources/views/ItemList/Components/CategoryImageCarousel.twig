<script type="x/template" id="vue-category-image-carousel">
    <a :id="'owl-carousel-' + _uid" v-if="$_enableCarousel" class="owl-carousel owl-theme test-1052" :href="itemUrl">
        <div v-for="(imageUrl, index) in imageUrls" :key="index">
            <lazy-img v-if="index === 0 && !disableLazyLoad" ref="itemLazyImage" picture-class="img-fluid" :image-url="imageUrl.url" :alt="getAltText(imageUrl)" :title="getImageName(imageUrl)"></lazy-img>
            <img v-else-if="index !== 0 && !disableLazyLoad" class="img-fluid owl-lazy" :data-src="imageUrl.url" :alt="getAltText(imageUrl)" :title="getImageName(imageUrl)">
            <img v-else class="img-fluid" :src="imageUrl.url" :alt="getAltText(imageUrl)" :title="getAltText(imageUrl)">
        </div>
    </a>

    <a v-else :href="itemUrl" class="test-1218">
      <div v-for="(imageUrl, index) in imageUrls" :key="index" v-if="index === 0 || index === 1">
        <lazy-img v-if="!disableLazyLoad" ref="itemLazyImage" picture-class="img-fluid" :picture-class="index" :image-url="imageUrl.url" :alt="getAltText(imageUrl)" :title="getImageName(imageUrl)"></lazy-img>
        <img v-else class="img-fluid test-1428" :src="imageUrls | itemImage" :alt="getAltText(imageUrl)" :title="getImageName(imageUrl)">
      </div>
    </a>
</script>
